  },
  "dependencies": {
    "@cypress/xpath": "^2.0.3",
    "@faker-js/faker": "^8.0.1",
    "cypress-wait-until": "^1.7.2"
  },
  "keywords": [],
  "directories": {
    "doc": "doc"
  }
